Resources:
    DBSubnetGroup:
        Type: AWS::RDS::DBSubnetGroup
        DBSubnetGroupDescription: "Subnet group for eb-rds-prod"
        Properties:
            SubnetIds: "subnet-0cf3e064, subnet-e1306d9b"
    AWSEBRDSDatabase:
        Type: AWS::RDS::DBInstance
        Properties:
            AllocatedStorage: 5
            DBInstanceClass: db.t2.micro
            DBName: eb-rds-prod
            Engine: MySQL
            EngineVersion: 5.7.17
            MasterUsername: eb-prod
            MasterUserPassword: Pass.123
            MultiAZ: "false"
            PubliclyAccessible: "false"
            MonitoringInterval: 60
            DBSubnetGroupName: "DBSubnetGroup"
            DeletionPolicy: "Delete"
            BackupRetentionPeriod: 0
            StorageType: standard
